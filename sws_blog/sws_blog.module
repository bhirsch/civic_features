<?php

include_once('sws_blog.features.inc');


/**
 * Implementation of hook_init().
 */
function sws_blog_init(){
  // Include custom css (this file gets written and updated when users create buttons)
  $file_path = drupal_get_path('module', 'sws_blog') .'/sws_blog.css';
  drupal_add_css($file_path);
}

